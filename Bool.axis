package axis.lang



//包级别类 <<<<<<<<<<<<<<<<<<<<<<

Any Bool { 

    final Str TRUE : 'true'
    final Str FALSE : 'false'

    @Override
    Str toStr() {
      return this ? TRUE ; FALSE
    }

    Bool OF(Str s) {
      s = s ?? ''
      return Bool{
        value : s = TRUE ? true ; false 
      }
    }
}

//包级别类 >>>>>>>>>>>>>>>>>>>>>>
