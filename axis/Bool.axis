//包级别类 <<<<<<<<<<<<<<<<<<<<<<

/**
 对应 Java 的 Boolean
 */
Any Bool : Booleanable { 

    final Str TRUE : 'true'
    final Str FALSE : 'false'

    //IDE 自动索引并显示左侧的上下箭头，不用写，导出会自动加上 @Override
    Str toStr() {
      return this ? TRUE ; FALSE
    }

    Bool OF(Str s) {
      s = s ?? ''
      return Bool{
        value : s = TRUE ? true ; false 
      }
    }


    Bool equal(Any a) {
      return a instanceof Booleanable & super.equal(/* Any a : */ a)
    }

}

//包级别类 >>>>>>>>>>>>>>>>>>>>>>
